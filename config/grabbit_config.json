{
  "entities": [
      {
        "name": "subject",
        "pattern": "(sub-[hp][cp]\\d+)",
        "directory": "{{root}}/{subject}"
      },
      {
        "name": "modality",
        "pattern": "(eeg|func)",
        "directory": "{{root}}/{subject}/{modality}"
      },
      {
        "name": "derivative",
        "pattern": "(fmriprep|freesurfer|mriqc|eeg_preprocessing)",
        "directory": "{{root}}/derivatives/{derivative}"
      }
  ]
}
